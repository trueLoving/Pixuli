{
  "name": "pixuli-app",
  "productName": "Pixuli",
  "version": "1.3.0",
  "type": "module",
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://localhost:5173"
    }
  },
  "description": "Pixuli App - 智能图片管理应用（支持 Web 和 Desktop）",
  "main": "dist-electron/main/index.js",
  "homepage": "./",
  "scripts": {
    "dev:web": "vite --mode web",
    "dev:desktop": "vite --mode desktop",
    "build:web": "vite build --mode web",
    "build:desktop": "tsc && vite build --mode desktop && electron-builder",
    "test": "vitest"
  },
  "keywords": ["electron", "vite", "react", "typescript", "图片管理"],
  "author": "trueLoving",
  "license": "MIT",
  "dependencies": {
    "i18next": "^25.6.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "lucide-react": "^0.556.0",
    "octokit": "3.1.2",
    "pixuli-common": "workspace:*",
    "pixuli-wasm": "workspace:*",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^16.0.1",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@types/node": "^22.18.0",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.0",
    "@vite-pwa/assets-generator": "^1.0.2",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.14",
    "electron": "^33.4.11",
    "electron-builder": "^24.0.0",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vite-plugin-pwa": "^1.1.0"
  }
}
