# Pixuli Web 环境变量配置示例
# 复制此文件为 .env.local 并根据需要修改配置

# 演示模式设置
# 设置为 true 启用演示模式，false 禁用演示模式
VITE_DEMO_MODE=false

# GitHub 演示配置（仅在演示模式下使用）
VITE_DEMO_GITHUB_OWNER=''
VITE_DEMO_GITHUB_REPO=''
VITE_DEMO_GITHUB_BRANCH=''
VITE_DEMO_GITHUB_TOKEN=''
VITE_DEMO_GITHUB_PATH=''

# Gitee 演示配置（仅在演示模式下使用）
VITE_DEMO_GITEE_OWNER=''
VITE_DEMO_GITEE_REPO=''
VITE_DEMO_GITEE_BRANCH=''
VITE_DEMO_GITEE_TOKEN=''
VITE_DEMO_GITEE_PATH=''

# Gitee 配置

#  Gitee 代理服务器（针对访问浏览器访问Gitee静态图片资源会出现跨域问题）
VITE_USE_GITEE_PROXY = true

# DevTools 调试工具
# 设置为 true 启用 DevTools 调试工具（右下角浮球），false 禁用
VITE_ENABLE_DEVTOOLS=false
