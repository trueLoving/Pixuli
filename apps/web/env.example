# Pixuli Web 环境变量配置示例
# 复制此文件为 .env.local 并根据需要修改配置

# 演示模式设置
# 设置为 true 启用演示模式，false 禁用演示模式
VITE_DEMO_MODE=false

# GitHub 演示配置（仅在演示模式下使用）
VITE_DEMO_GITHUB_OWNER=''
VITE_DEMO_GITHUB_REPO=''
VITE_DEMO_GITHUB_BRANCH=''
VITE_DEMO_GITHUB_TOKEN=''
VITE_DEMO_GITHUB_PATH=''


# Gitee 配置

#  Gitee 代理服务器
VITE_USE_GITEE_PROXY = true
